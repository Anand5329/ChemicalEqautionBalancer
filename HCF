int HCF(int[][] arr)
    {
        int max=0;
        boolean flag=true;
        for(int i=0;i<arr.length;i++)
        {
            flag=true;
            for(int j=0;j<arr[0].length;j++)
            {
                if(arr[i][j]!=1)
                {
                    flag=false;
                    break;
                }
            }
            if(flag)
                max=i+1;
        }
        return max;
    }
